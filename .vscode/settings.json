{
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "editor.defaultFormatter": "espenp.prettier-vscode",
      "source-organizeImports": true
    }
  },
  "editor.formatOnSave": true,
  "cSpell.enableFiletypes": [
    "javascript"
  ],
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.words": [
    "Parens"
  ]
}